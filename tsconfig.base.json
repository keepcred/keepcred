{
  "compilerOptions": {
    "paths": {
      "#/main/*": ["./src/main/*"],
      "#/shared/*": ["./src/shared/*"],
      "#/preload/*": ["./src/preload/*"],
      "#/renderer/*": ["./src/renderer/*"],
      "#/*": ["./src/*"]
    },
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "target": "esnext",
    "module": "esnext",
    "sourceMap": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true
  }
}
